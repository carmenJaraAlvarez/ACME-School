		<property name="folders">
			<list>
			</list>
		</property>
		<property name="messagesSent">
			<list>
			</list>
		</property>
		<property name="messagesReceived">
			<list>
			</list>
		</property>
		<property name="userAccount" ref="ua_administrator1" />
	</bean>

</beans>
